{
	"manifest_version": 2,
	"name": "Refined GSHS",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": [
				"http://student.gs.hs.kr/**"
			],
			"js": [
				"js/content.js"
			]
		}
	],
	"permissions": [
		"storage",
		"tabs"
	],
	"browser_action": {
		"default_popup": "html/popup.html",
		"default_icon": {
			"16": "img/favicon-16x16.png",
			"32": "img/favicon-32x32.png",
			"128": "img/favicon-128x128.png"
		},
		"default_title": "Refined GSHS"
	},
	"icons": {
		"16": "img/favicon-16x16.png",
		"32": "img/favicon-32x32.png",
		"128": "img/favicon-128x128.png"
	}
}
